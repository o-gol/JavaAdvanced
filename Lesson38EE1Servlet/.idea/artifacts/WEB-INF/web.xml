<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee"
           xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
           xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_4_0.xsd"
           version="2.5">
    <servlet>
        <servlet-name>firstServlet</servlet-name>
        <servlet-class>FirstServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>firstServlet</servlet-name>
        <url-pattern>/hello_world</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>secondServlet</servlet-name>
        <servlet-class>SecondServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>secondServlet</servlet-name>
        <url-pattern>/secondServlet_hello_world</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>firstJsp</servlet-name>
        <jsp-file>/firstJsp.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>firstJsp</servlet-name>
        <url-pattern>/jsp_hello_world</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>secondJsp</servlet-name>
        <jsp-file>/secondJsp.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>secondJsp</servlet-name>
        <url-pattern>/secondJsp_hello_world</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>JDBCJsp</servlet-name>
        <jsp-file>/jdbc.jsp</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>JDBCJsp</servlet-name>
        <url-pattern>/jdbc_jsp</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>Index</servlet-name>
        <jsp-file>/index.html</jsp-file>
    </servlet>
    <servlet-mapping>
        <servlet-name>Index</servlet-name>
        <url-pattern>/index</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>SetServlet</servlet-name>
        <servlet-class>сookies.SetServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SetServlet</servlet-name>
        <url-pattern>/setServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>GetServlet</servlet-name>
        <servlet-class>сookies.GetServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetServlet</servlet-name>
        <url-pattern>/getServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>DelServlet</servlet-name>
        <servlet-class>сookies.DelServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>DelServlet</servlet-name>
        <url-pattern>/delServlet</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>JDBSServlet</servlet-name>
        <servlet-class>jdbc.JDBSServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>JDBSServlet</servlet-name>
        <url-pattern>/jdbc</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>JDBSServlet2</servlet-name>
        <servlet-class>jdbc.JDBSServlet2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>JDBSServlet2</servlet-name>
        <url-pattern>/jdbc2</url-pattern>
    </servlet-mapping>

</web-app>
